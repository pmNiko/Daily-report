<%= form_for @profile do |f| %>
 <div class="form-group">
  <%= f.label :first_name %>
  <%= f.text_field :first_name, class: 'form-control' %>
 </div>
 <div class="form-group">
  <%= f.label :last_name %>
  <%= f.text_field :last_name, class: 'form-control' %>
 </div>
 <div class="form-group">
  <%= f.label :date_of_birth %>
  <%= f.date_field :date_of_birth, class: 'form-control' %>
 </div>
 <div class="panel panel-default">
    <div class="panel-heading">
      <h3 class="panel-title">Phones</h3>
    </div>
    <div class="panel-body">
      <div id="phones">
        <%= f.fields_for :phones do | phone | %>
          <%= render "phone_fields", :f => phone %>
        <% end %>
      </div>
        <%= link_to_add_association 'Add Phone', f, :phones %>
      </div>
    </div>
  </div>
  <button type="button" name="button" class= "btn btn-default" >
    <%= f.submit %>
  </button>
<% end %>
